<table>
  <% for(var i=0; i < data.length; i++) { %>
    <tr>
      <td><%= data[i].name %></td>
    </tr>
    <% for(var j=0; j < data[i].values.length; j++) { %>
      <tr>
        <td class=graph-bar-cell>
          <div class="graph-bar <%= 'graph-bar-' + j %>" style="width: 0px" value=<%= data[i].values[j] %>></div>
        </td>
        <td><%= data[i].values[j] %></td>
      </tr>
    <% } %>
  <% } %>
</table>