  <div class=base-menu>
    <div class=ui-multiselect-menu>
      <ul class=ui-helper-reset>
        <li>
          <a class="ui-multiselect-all" href="#">
            <span class="ui-icon ui-icon-check">&#x2714;</span>
            <span>Check all</span>
          </a>
        </li>
        <li>
          <a class="ui-multiselect-none" href="#">
            <span class="ui-icon ui-icon-closethick">&#x2716;</span>
