<div>
  <div class=controls>
    <div class=app-logo></div>
  </div>

  <a href="<%= data.logoHref || '/' %>" class=logo></a>

  <div class=tabs>
    <% _.each(data.tabs, function(tab) { %>
      <a href="<%= _.isObject(tab)? tab.href : 'javascript:void(0)' %>" class=tab>
        <%= _.isFunction(tab)? tab() : _.isObject(tab)? tab.content : tab %>
      </a>
    <% }); %>
  </div>

</div>
